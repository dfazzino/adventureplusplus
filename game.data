					eventDone = loadVariable(line)
				end
				
					
			end
		until line == nil
	
	end

end

function loadVariable(line)
	
	words = line:split(" ")
	
	for i, word in ipairs(words)  do
		--first word should be new
		--logFile:write(word .. "\n")
		word = word:strip()
		if word == "new" then
			setupNewVariable()
			word = nil
		end
		
		if word ~= nil then
			word = word:strip()
			--print(word)
			if word:startsWith("variable.") then
				--print(word)
				word = word:gsub("%.", "|")
				variableWords = split(word, "|")
				-- print (entityWords[2])
				if variableWords[2] == "name" then
					variable.name = words[i + 2]
				end
				if variableWords[2] == "value" then
					variable.value = words[i + 2]
				end
			end
		end
		if word == "add" then
			addVariable()
			logFile:flush()
			return true
			
		end
		if word == "new" then
			return true
		end
	end
	return false
	
	
end


