	-- Iterator, wrapped within a coroutine
	-- Iterates around a given position following the outline of a square
	local function around()
		local iterf = function(x0, y0, s)
			local x, y = x0-s, y0-s
			coroutine.yield(x, y)
			repeat
				x = x + 1
				coroutine.yield(x,y)
			until x == x0+s
			repeat
				y = y + 1
				coroutine.yield(x,y)
			until y == y0 + s
			repeat
				x = x - 1
				coroutine.yield(x, y)
			until x == x0-s
			repeat
				y = y - 1
				coroutine.yield(x,y)
			until y == y0-s+1
		end
